$(document).ready(function(){
	$('#btnNext').on('click', function(){
		$('.reg-1').hide();
		$('.reg-2').show();
	});
	$('#btnReturn').on('click',function(){
		$('.reg-1').show();
		$('.reg-2').hide();
	});
});


