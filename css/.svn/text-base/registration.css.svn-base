/*!
 * header component
 */
.header {
  height: 100px;
  border-bottom: 1px solid #A0A0A0;
}
.header .container {
  height: 100%;
}
.header .container .logo {
  height: 43px;
  width: 150px;
  position: relative;
  top: 33px;
  background-image: url(../img/header-logo.png);
}
.header .container .logo a {
  display: block;
  height: 100%;
  width: 100%;
}
.header .container .nav {
  margin-top: 0;
  position: absolute;
  right: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
          transform: translateY(-50%);
}
.header .container .nav li {
  float: left;
  padding-left: 0;
  margin-left: 80px;
  list-style: none;
  font-size: 16px;
  color: #a0a0a0;
}
.header .container .nav li a {
  padding-bottom: 0;
}
.header .container .nav li a:hover,
.header .container .nav li .active {
  color: #000000;
  padding-bottom: 8px;
  border-bottom: 2px solid #000000;
  transition-duration: 0.3s;
}
/*!
 * footer component
 */
.footer {
  margin-top: 80px;
  padding-top: 32px;
  font-size: 14px;
  background-color: #404040;
  color: #FFFFFF;
}
.footer .left {
  float: left;
  width: 60%;
}
.footer .left a:hover {
  text-decoration: underline;
}
.footer .left .nav {
  margin-top: 0;
  padding-left: 0;
}
.footer .left .nav li {
  float: left;
  list-style: none;
  margin-right: 24px;
}
.footer .left .nav li:after {
  content: "";
  display: inline-block;
  position: relative;
  top: 2px;
  height: 14px;
  width: 1px;
  margin-left: 24px;
  background-color: #FFFFFF;
}
.footer .left .nav li:last-child:after {
  display: none;
}
.footer .left .blogroll {
  clear: both;
  padding-top: 22px;
  line-height: 24px;
}
.footer .left .blogroll dt {
  float: left;
}
.footer .left .blogroll dd {
  margin-left: 70px;
}
.footer .right {
  width: 40%;
  float: right;
}
.footer .right .qr-code {
  float: left;
  height: 110px;
  width: 110px;
  margin-right: 18px;
  background-color: #FFFFFF;
}
.footer .right .contactway {
  position: relative;
  top: -6px;
  margin-top: 0;
  line-height: 26px;
}
.footer .right .contactway dt {
  float: left;
}
.footer .right .contactway dl {
  margin-top: 0;
  margin-bottom: 0;
}
.footer .copyright {
  clear: both;
  margin: 0;
  padding-top: 30px;
  padding-bottom: 20px;
  opacity: 0.5;
  text-align: center;
}
