  text-decoration: none;
}
.bg {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  min-width: 1000px;
  background: url(../img/entry-bg.jpg) center no-repeat;
  background-size: cover;
}
.esee-model {
  float: left;
}
.esee-model h2 {
  color: #FFFFFF;
}
.esee-kid {
  float: right;
}
.esee-kid h2 {
  color: #000000;
}
.esee-model,
.esee-kid {
  position: relative;
  width: 50%;
  height: 100%;
}
.esee-model a,
.esee-kid a {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  display: block;
  height: 290px;
  width: 360px;
  text-align: center;
}
.esee-model a img,
.esee-kid a img {
  position: relative;
  top: 0;
  height: 80px;
  margin-bottom: 40px;
}
.esee-model a h2,
.esee-kid a h2 {
  position: relative;
  top: 0;
  margin-top: 0;
  margin-bottom: 80px;
  font-size: 26px;
  font-weight: normal;
}
.esee-model a p,
.esee-kid a p {
  height: 40px;
  width: 120px;
  margin: 0 auto;
  color: #A0A0A0;
  font-size: 18px;
  line-height: 40px;
}
.esee-model a:hover img,
.esee-kid a:hover img {
  top: -10px;
  transition-duration: 0.5s;
}
.esee-model a:hover h2,
.esee-kid a:hover h2 {
  top: -10px;
  transition-duration: 0.5s;
}
.esee-model a:hover p,
.esee-kid a:hover p {
  line-height: 40px;
  color: #fff;
  background: #CBA06B;
  box-shadow: 5px 5px 5px rgba(0, 0, 0, 0.3);
  transition-duration: 0.5s;
}
