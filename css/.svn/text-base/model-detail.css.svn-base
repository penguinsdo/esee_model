  top: 50%;
  left: 0;
  -webkit-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
          transform: translateY(-50%);
  width: 100%;
}
.model-detail .model-information {
  position: absolute;
  top: 50px;
  left: 674px;
}
.model-detail .model-information h3 {
  margin: 0;
  font-size: 40px;
  font-weight: normal;
}
.model-detail .model-information h3:after {
  content: "";
  display: block;
  height: 2px;
  width: 56px;
  margin-top: 24px;
  background-color: #000000;
}
.model-detail .model-information dl {
  position: relative;
  left: 80px;
  top: -6px;
  width: 440px;
