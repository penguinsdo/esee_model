}
.footer .right .qr-code {
  float: left;
  height: 110px;
  width: 110px;
  margin-right: 18px;
  background-color: #FFFFFF;
}
.footer .right .contactway {
  position: relative;
  top: -6px;
  margin-top: 0;
  line-height: 26px;
}
.footer .right .contactway dt {
  float: left;
}
.footer .right .contactway dl {
  margin-top: 0;
  margin-bottom: 0;
}
.footer .copyright {
  clear: both;
